# Telegram Bot Token (从 @BotFather 获取)
BOT_TOKEN=your_bot_token_here

# 你的 Telegram Chat ID (可以通过 @userinfobot 获取)
CHAT_ID=your_chat_id_here

# Web 管理面板端口 (默认 3000)
WEB_PORT=3000

# Web 管理面板监听地址 (默认 127.0.0.1 仅本地访问，设置为 0.0.0.0 允许局域网访问)
WEB_HOST=127.0.0.1

# RSS 检查间隔 (分钟)
CHECK_INTERVAL=10

# 数据保留天数 (天)
RETENTION_DAYS=30

# 是否启用调试日志 (true/false)
DEBUG=false
